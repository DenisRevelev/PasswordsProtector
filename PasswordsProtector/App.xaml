<Application x:Class="PasswordsProtector.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PasswordsProtector"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/ResourceDictionary/ColorsDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionary/ButtonsDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionary/TextBoxDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionary/TextBlockDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionary/ListViewDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionary/ComboBoxDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
